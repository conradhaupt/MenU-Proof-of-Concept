<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <!--
        Base application theme, dependent on API level. This theme is replaced
        by AppBaseTheme from res/values-vXX/styles.xml on newer devices.
    -->


    <!-- this style is only referenced in a Light.DarkActionBar based theme -->

    <style name="holo_light_darkactionbar" parent="@android:style/Theme.Holo.Light.DarkActionBar">
        <item name="ic_menu_moreoverflow">@drawable/ic_menu_moreoverflow_normal_holo_dark</item>
        <item name="ic_menu_search_holo">@drawable/ic_menu_search_holo_dark</item>
        <item name="slidingmenu_background">@drawable/slidingmenu_holo_light_darkactionbar_background</item>
    </style>

    <style name="holo" parent="@android:style/Theme.Holo">
        <item name="ic_menu_moreoverflow">@drawable/ic_menu_moreoverflow_normal_holo_dark</item>
        <item name="ic_menu_search_holo">@drawable/ic_menu_search_holo_dark</item>
        <item name="slidingmenu_background">@drawable/slidingmenu_holo_light_darkactionbar_background</item>
    </style>

    <style name="holo_light" parent="@android:style/Theme.Holo.Light">
        <item name="ic_menu_moreoverflow">@drawable/ic_menu_moreoverflow_normal_holo_light</item>
        <item name="ic_menu_search_holo">@drawable/ic_menu_search_holo_light</item>
        <item name="slidingmenu_background">@drawable/slidingmenu_holo_light_background</item>
    </style>

</resources>